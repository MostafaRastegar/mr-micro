{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "declaration": true,
    "declarationMap": true,
    "noImplicitAny": true,
    "sourceMap": false,
    "lib": ["es6"],
    "baseUrl": ".",
    "paths": {
      "@mfTypes/*": ["./@mf-types/Remote/_types/*"]
    }
  }
}
